{
    "name": "jonasarts/phpqrcode-bundle",
    "description": "A PHP QR Code Bundle for Symfony",
    "keywords": ["QR", "Code"],
    "homepage": "https://github.com/jonasarts/phpqrcode-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hauser",
            "email": "symfony@jonasarts.com",
            "homepage": "http://www.jonasarts.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "chillerlan/php-qrcode": "^4.3||^5.0"
    },
    "autoload": {
        "psr-4": {
            "jonasarts\\Bundle\\PHPQRCodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jonasarts\\Bundle\\PHPQRCodeBundle\\Tests\\": "tests/"
        }
    }
}
